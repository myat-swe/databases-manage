CREATE USER IF NOT EXISTS 'onestop_user'@'localhost' IDENTIFIED WITH mysql_native_password BY 'onestoppass';

GRANT ALL //PRIVILEGES// ON jdbc_db.* TO 'onestop_user'@'localhost';

FLUSH PRIVITEGES;

select user,host from mysql.user;//eg .user name select

