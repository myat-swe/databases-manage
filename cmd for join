

select s.name,s.gender,c.phone,c.email,replace(a.location, "Township", "")location,r.reg_fees,r.reg_date,cou.name,cou.fees,cou.level from student s join contact c on s.contact_id = c.phone join address a on s.address_id = a.id join registration r on s.id = r.student_id join course cou on cou.id = r.course_id;
select s.name,s.gender,c.phone,c.email,replace(a.location, "Township", "")location,r.reg_fees,r.reg_date,cou.name,cou.fees,cou.level from student s join contact c on s.contact_id = c.phone join address a on s.address_id = a.id join registration r on s.id = r.student_id join course cou on cou.id = r.course_id limit 10;
select s.name,s.gender,c.phone,c.email,replace(a.location, "Township", "")location,r.reg_fees,r.reg_date,cou.name,cou.fees,cou.level from student s join contact c on s.contact_id = c.phone join address a on s.address_id = a.id join registration r on s.id = r.student_id join course cou on cou.id = r.course_id limit 20,10;
select s.name,s.gender,c.phone,c.email,replace(a.location, "Township", "")location,r.reg_fees,r.reg_date,cou.name,cou.fees,cou.level from student s join contact c on s.contact_id = c.phone join address a on s.address_id = a.id join registration r on s.id = r.student_id join course cou on cou.id = r.course_id limit 70,10; //note id 70 => 10 name;
select s.name,s.gender,c.phone,c.email,replace(a.location, "Township", "")location,r.reg_fees,r.reg_date,cou.name,cou.fees,cou.level from student s join contact c on s.contact_id = c.phone join address a on s.address_id = a.id join registration r on s.id = r.student_id join course cou on cou.id = r.course_id order by cou.name limit 10;
mysql> select s.name,s.gender,c.phone,c.email,replace(a.location, "Township", "")location,r.reg_fees,r.reg_date,cou.name,cou.fees,cou.level from student s join contact c on s.contact_id = c.phone join address a on s.address_id = a.id join registration r on s.id = r.student_id join course cou on cou.id = r.course_id order by cou.name desc, r.reg_date asc limit 0,10;

select name,fees,level,start_date, duration_in_hour from course where fees >(select avg(fees) from course);

mysql> select name from module m  where m.course_id = (select c.id from course c where lower(c.name) =  lower('spring framework basics'));

select m.name from module m where m.course_id in(select c.id from course c where lower(c.name) in (lower('spring framework basics'),lower('spring boot microservice')));
select name from module m  where m.course_id in (select c.id from course c where lower(c.name) like lower('%spring%'));
how many students have in that course;
select course_id, count(student_id) from registration group by course_id;
select c.name,r.course_id, count(r.student_id) from registration r join course c on c.id = r.course_id group by course_id;
select c.name,r.reg_date,r.course_id, count(r.student_id) from registration r join course c on c.id = r.course_id group by course_id, r.reg_date;

select c.name,c.level,count(r.student_id) from registration r join course c on c.id = r.course_id group by c.name ,c.level;

select c.name, sum(r.reg_fees) total from registration r join course c on c.id = r.course_id group by c.name;

select m.course_id,c.id,count(name) from module m join course c on c.id = m.course_id group by c.id;

error select m.course_id c.id, c.name, count(m.id) from module m join course c on c.id = m.course_id group by c_id;

select c.name, sum(r.reg_fees) total from registration r join course c on c.id = r.course_id where c.name like('%java%') OR c.name like('%spring%') group by c.name having total>=500000;


