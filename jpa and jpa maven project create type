Elementary Mapping //wrapper type -> null;
//primitive type -> not  null;
byte  -> tinyint  not null; 
short -> smallintnnot null;
int   -> integernnot null;
float -> folat(23n)not null;
double -> float(5n3)not null;
char  -> char(1)n
boolean -> bitnnot null;
String -> varchar(255)
BigInteger -> decimal(38,0)
BigDecimal -> decimal(38,2)

array[]  type char -> varchar(255)
all array[] type -> varbinary();




