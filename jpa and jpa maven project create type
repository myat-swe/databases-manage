@Column 
name -> override column name (dafault  field name);
unique -> override unuque constraint(default false);
nullable : boolean -> set null or not null(default true);
columnDefinition String -> set vendor specific sql column definition (not recommend).
length : int -> only for String ;
precision int -> only for exact numeric value(biginteger, bigdecimal),  for integer part;
scale : int -> only for exact numeric value(biginteger, bigdecimal), for decimal part;
